#include <stdio.h>
#include <stdlib.h>
#include "mylib.h"
#include "htable.h"

int main(int argc, char **argv) {

    int table_size = 18143;
    htable h;
    char word[256];
    
    if (argc > 1) {
        table_size = atoi(argv[1]);
    }

    
    h = htable_new(table_size);
    
    while (getword(word, sizeof word, stdin) != EOF) {
        htable_insert(h, word);
    }
    
    htable_print(h, stdout);
    htable_free(h);

    printf("%d\n", table_size);
    
    return EXIT_SUCCESS;
 }
